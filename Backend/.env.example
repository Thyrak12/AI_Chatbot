# Backend environment variables (DO NOT put real secrets in this file)

# Server
PORT=5000

# MongoDB (required)
# Example (local): mongodb://127.0.0.1:27017
# Example (Atlas):  mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority
MONGO_URI=

# Gemini (optional)
# Either of these is accepted by the backend
GEMINI_API_KEY=
GENAI_API_KEY=
GENAI_MODEL=gemini-2.5-flash-lite

# DeepSeek (optional)
DEEPSEEK_API_KEY=
DEEPSEEK_MODEL=deepseek-chat
